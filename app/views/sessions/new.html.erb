<div class = "login-container">
  <%= form_for @user, url: "/sessions", method: "post" do |f| %>
  <div>
    <%= f.text_field :user_name, placeholder: "Username" %>
  </div>
  <div>
    <%= f.password_field :password, placeholder: "Password" %>
  </div>
  <%= f.submit "Log In" %>
  <% end %>
</div>
